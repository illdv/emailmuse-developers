{
  "name": "EmailMuse",
  "version": "0.3.21",
  "author": {
    "name": "Vyacheslav Sinyakov",
    "email": "vyacheslav.sinyakov@gmail.com"
  },
  "homepage": "homepage.example",
  "description": "EmailMuse",
  "main": "./main.js",
  "scripts": {
    "start-webpack-dev-server": "webpack-dev-server",
    "start-electron": "npx electron ./src/main/index.ts",
    "build": "rm -rf build; NODE_ENV=production  webpack",
    "build-prod": "npm run build; cd build; npx electron-builder -wlmp always",
    "build:linux": "npm run build; cd build; npx electron-builder -l",
    "build:mac": "npm run build; cd build; npx electron-builder -m",
    "release": ". ./setEnvVar.sh && cd build && npx electron-builder -p always",
    "test": "jest",
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "project-preparation": "rm -rf node_modules build dist; kill $(lsof -t -i:8080); npm i",
    "cy": "cypress open",
    "cy:all": "cypress --record",
    "cy:re": "cypress run --record --parallel --key --ci-build-id test"
  },
  "dependencies": {
    "@material-ui/core": "^1.3.0",
    "@material-ui/icons": "^1.1.0",
    "@types/react-joyride": "^2.0.1",
    "@types/react-router-dom": "^4.2.7",
    "axios": "^0.18.0",
    "bem-ts": "^0.6.0",
    "classnames": "^2.2.5",
    "electron-is-dev": "^1.0.1",
    "jodit": "https://github.com/illdv/jodit/tarball/master",
    "log4js": "^3.0.6",
    "moment-es6": "^1.0.0",
    "react": "^16.3.2",
    "react-color": "^2.14.1",
    "react-dnd": "^2.6.0",
    "react-dnd-html5-backend": "^2.6.0",
    "react-dom": "^16.3.2",
    "react-joyride": "^2.0.0-15",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.4.0-alpha.1",
    "react-router-redux": "^4.0.8",
    "redux": "^3.7.2",
    "redux-act": "^1.7.4",
    "redux-actions": "^2.3.2",
    "redux-logger": "^3.0.6",
    "redux-saga": "^0.16.0",
    "reselect": "^3.0.1",
    "resolve": "^1.6.0",
    "validate.js": "^0.12.0"
  },
  "devDependencies": {
    "@cypress/snapshot": "^2.0.1",
    "@types/dotenv": "^6.1.0",
    "@types/enzyme": "^3.1.10",
    "@types/jest": "^23.1.0",
    "@types/node": "^10.1.2",
    "@types/react": "^16.3.14",
    "@types/react-dom": "^16.0.5",
    "electron": "^3.0.10",
    "electron-builder": "^20.36.2",
    "electron-log": "^2.2.17",
    "electron-updater": "^4.0.4",
    "awesome-typescript-loader": "^5.0.0",
    "copy-webpack-plugin": "^4.5.1",
    "css-loader": "^0.28.11",
    "cypress": "^3.1.0",
    "dotenv": "^6.0.0",
    "electron-load-devtool": "^1.0.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-context-patch": "0.0.8",
    "enzyme-to-json": "^3.3.4",
    "eslint": "^4.19.1",
    "eslint-plugin-react": "^7.8.2",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^22.4.4",
    "jest-webpack-resolver": "^0.3.0",
    "mini-css-extract-plugin": "^0.4.0",
    "node-env-run": "^3.0.2",
    "node-sass": "^4.9.0",
    "react-addons-test-utils": "^15.6.2",
    "react-dev-utils": "^6.0.5",
    "react-test-renderer": "^16.4.1",
    "redux-devtools": "^3.4.1",
    "redux-devtools-extension": "^2.13.2",
    "redux-mock-store": "^1.5.1",
    "sass-loader": "^7.0.1",
    "source-map-loader": "^0.2.3",
    "style-loader": "^0.21.0",
    "ts-jest": "^22.4.6",
    "tslint": "^5.10.0",
    "tslint-react": "^3.6.0",
    "typescript": "^2.8.3",
    "url-loader": "^1.0.1",
    "webpack": "^4.8.3",
    "webpack-cli": "^3.0.7",
    "webpack-dev-server": "^3.1.10"
  },
  "build": {
    "productName": "EmailMuse",
    "appId": "jack.and.slava.emailmuse",
    "publish": {
      "provider": "s3",
      "bucket": "emailmuse"
    },
    "linux": {
      "category": "Office"
    },
    "mac": {
      "category": "public.app-category.utilities"
    }
  },
  "jest": {
    "jestWebpackResolver": {
      "webpackConfig": "<rootDir>/webpack.config.js"
    }
  }
}
